  "private": true,
  "dependencies": {
    "@equinor/eds-core-react": "^0.37.0",
    "@equinor/eds-icons": "^0.21.0",
    "@equinor/eds-tokens": "^0.9.2",
    "@microsoft/applicationinsights-react-js": "^17.1.2",
    "@microsoft/applicationinsights-web": "^3.2.0",
    "axios": "^1.6.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "react-oauth2-code-pkce": "^1.18.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.9"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "typescript": "^5.4.5"
  },
  "overrides": {
    "react-scripts": {
      "typescript": "^5.2.2"
    }
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN='true' WDS_SOCKET_PORT=80 react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test": "react-scripts test",
    "format": "yarn dlx -q @biomejs/biome format src --write",
    "lint": "yarn dlx -q @biomejs/biome lint src --apply",
    "check": "yarn dlx -q @biomejs/biome check src --apply",
    "test-ci": "react-scripts test --watchAll=false",
    "upgrade": "yarn upgrade-interactive && yarn up && yarn dedupe",
    "eject": "react-scripts eject"
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.5.1"
}
